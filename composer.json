{
    "name": "cakedc/cakephp-money",
    "description": "CakeDC/Money plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "~4.2.0",
        "moneyphp/money": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "CakeDC\\Money\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeDC\\Money\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
